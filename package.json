{
    "name": "orbyt",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "android": "react-native run-android",
        "ios": "react-native run-ios",
        "postinstall": "rn-nodeify --install fs,dgram,process,path,console,stream,crypto --hack",
        "start": "react-native start",
        "test": "jest",
        "prettier:write": "npx prettier --write **/*.{js,jsx,ts,tsx,json} && npx prettier --write *.{js,jsx,ts,tsx,json}",
        "lint": "tsc --noEmit && eslint --ext .js,.jsx,.ts,.tsx ./",
        "prettier:check": "npx prettier --check apps/",
        "run:android": "yarn prettier:check && yarn prettier:write && npx react-native run-android",
        "beta:android": "cd ./android && bundle exec fastlane beta",
        "release:android": "cd ./android && bundle exec fastlane release"
    },
    "eslintConfig": {
        "extends": "universe/native"
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged && yarn lint"
        }
    },
    "dependencies": {
        "@ethersproject/shims": "^5.7.0",
        "@klarna/react-native-vector-drawable": "^0.3.0",
        "@react-native-async-storage/async-storage": "^1.17.5",
        "@react-native-firebase/app": "^15.1.1",
        "@react-navigation/bottom-tabs": "^6.3.1",
        "@react-navigation/native": "^6.0.8",
        "@react-navigation/native-stack": "^6.5.0",
        "@robinbobin/react-native-google-drive-api-wrapper": "^1.2.3",
        "@toruslabs/react-native-web-browser": "^1.1.0",
        "@walletconnect/react-native-dapp": "^1.7.8",
        "@web3auth/react-native-sdk": "^3.2.1",
        "@web3auth/web3auth": "^2.1.3",
        "add": "^2.0.6",
        "alchemy-sdk": "^2.2.5",
        "base-64": "^1.0.0",
        "buffer": "^6.0.3",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-config-universe": "^11.1.1",
        "eslint-plugin-react-native": "^4.0.0",
        "ethers": "^5.6.8",
        "global": "^4.4.0",
        "prettier": "^2.8.1",
        "pretty-quick": "^3.1.3",
        "process": "^0.11.10",
        "react": "^18.1.0",
        "react-crypto-icons": "^1.0.5",
        "react-dom": "^18.1.0",
        "react-native": "0.67.3",
        "react-native-crypto-icons": "^1.0.1",
        "react-native-dotenv": "^3.3.1",
        "react-native-fs": "^2.20.0",
        "react-native-level-fs": "^3.0.0",
        "react-native-logs": "^5.0.1",
        "react-native-qrcode": "^0.2.7",
        "react-native-qrcode-svg": "^6.2.0",
        "react-native-randombytes": "^3.6.1",
        "react-native-reanimated": "2.2.4",
        "react-native-safe-area-context": "^4.1.2",
        "react-native-screens": "^3.13.1",
        "react-native-splash-screen": "^3.3.0",
        "react-native-svg": "9.6.4",
        "react-native-text-gradient": "^0.1.7",
        "react-native-udp": "^2.1.0",
        "react-native-vector-icons": "^9.1.0",
        "react-native-vector-image": "^0.3.3",
        "react-native-webview": "^11.26.0",
        "react-redux": "^7.2.6",
        "readable-stream": "1.0.33",
        "redux-persist": "^6.0.0",
        "redux-thunk": "^2.4.1",
        "rn-nodeify": "^10.3.0",
        "stream-browserify": "^1.0.0",
        "ts-jest": "^28.0.4",
        "web3": "^1.8.1",
        "yarn": "^1.22.18"
    },
    "devDependencies": {
        "@babel/core": "^7.12.9",
        "@babel/runtime": "^7.12.5",
        "@react-native-community/eslint-config": "^3.2.0",
        "@types/": "react-navigation/native",
        "@types/jest": "^28.1.1",
        "@types/node": "^17.0.40",
        "@types/react": "^18.0.12",
        "@types/react-native": "^0.67.8",
        "@types/react-native-dotenv": "^0.2.0",
        "@types/react-native-vector-icons": "^6.4.10",
        "@types/react-test-renderer": "^17.0.1",
        "@typescript-eslint/eslint-plugin": "^5.48.0",
        "@typescript-eslint/parser": "^5.7.0",
        "babel-jest": "^26.6.3",
        "babel-plugin-module-resolver": "^4.1.0",
        "enzyme": "^3.11.0",
        "enzyme-adapter-react-16": "^1.15.6",
        "eslint": "^8.31.0",
        "eslint-config-airbnb-typescript": "^17.0.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jsx-a11y": "^6.6.1",
        "eslint-plugin-react": "^7.31.11",
        "husky": "^8.0.3",
        "jest": "^28.1.0",
        "jsdom": "^19.0.0",
        "metro-react-native-babel-preset": "^0.66.2",
        "node-libs-react-native": "^1.2.1",
        "react-native-mock": "^0.3.1",
        "react-native-mock-render": "^0.1.9",
        "react-native-svg-transformer": "^1.0.0",
        "react-test-renderer": "^18.1.0",
        "typescript": "^4.7.3"
    },
    "jest": {
        "preset": "react-native",
        "roots": [
            "./__tests__"
        ],
        "moduleDirectories": [
            "node_modules"
        ],
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json",
            "node"
        ]
    },
    "react-native": {
        "console": "console-browserify",
        "crypto": "react-native-crypto",
        "path": "path-browserify",
        "fs": "react-native-level-fs",
        "_stream_transform": "readable-stream/transform",
        "_stream_readable": "readable-stream/readable",
        "_stream_writable": "readable-stream/writable",
        "_stream_duplex": "readable-stream/duplex",
        "_stream_passthrough": "readable-stream/passthrough",
        "dgram": "react-native-udp",
        "stream": "stream-browserify"
    },
    "browser": {
        "console": "console-browserify",
        "crypto": "react-native-crypto",
        "path": "path-browserify",
        "fs": "react-native-level-fs",
        "_stream_transform": "readable-stream/transform",
        "_stream_readable": "readable-stream/readable",
        "_stream_writable": "readable-stream/writable",
        "_stream_duplex": "readable-stream/duplex",
        "_stream_passthrough": "readable-stream/passthrough",
        "dgram": "react-native-udp",
        "stream": "stream-browserify"
    }
}
